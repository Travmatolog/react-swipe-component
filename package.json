{
  "name": "react-swipe-component",
  "version": "1.4.0",
  "description": "Swipe listener for react",
  "main": "lib/Swipe.min.js",
  "scripts": {
    "build": "babel src -d lib && babel ./lib/Swipe.js --out-file ./lib/Swipe.min.js --presets=babili",
    "prepare": "npm run build",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exelban/react-swipe-component.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "swipe",
    "swipe-left",
    "swipe-right",
    "swipe-up",
    "swipe-down",
    "swipe-listener",
    "listener",
    "swipeable",
    "react-component"
  ],
  "author": {
    "name": "Serhiy Mytrovtsiy",
    "email": "mitrovtsiy@ukr.net"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/exelban/react-swipe-component/issues"
  },
  "homepage": "https://github.com/exelban/react-swipe-component",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babili": "^0.1.4"
  }
